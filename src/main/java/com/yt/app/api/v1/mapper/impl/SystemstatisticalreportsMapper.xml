<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.yt.app.api.v1.mapper.SystemstatisticalreportsMapper">
	<!-- Base_Column_List -->
	<sql id="Base_Column_List">
		id,tenant_id,userid,merchantid,todayincome,incomecount,incomeuserpaycount,incomeuserpaysuccesscount,dateval,payoutrate,todayorder,successorder,todayorderamount,todaysuccessorderamount,type,create_by,create_time,update_by,update_time,remark,version
	</sql>
	<!-- ResultMap -->
	<resultMap id="ResultMap"
		type="com.yt.app.api.v1.entity.Systemstatisticalreports">
	</resultMap>
	<!-- ResultMap -->
	<resultMap id="ResultMapVO"
		type="com.yt.app.api.v1.vo.SystemstatisticalreportsVO">
	</resultMap>
	<!-- add -->
	<insert id="post"
		parameterType="com.yt.app.api.v1.entity.Systemstatisticalreports">
		insert into
		systemstatisticalreports(id,userid,merchantid,todayincome,incomecount,incomeuserpaycount,incomeuserpaysuccesscount,dateval,payoutrate,todayorder,successorder,todayorderamount,todaysuccessorderamount,type,create_by,create_time,update_by,update_time,remark)
		values
		(#{id},#{userid},#{merchantid},#{todayincome},#{incomecount},#{incomeuserpaycount},#{incomeuserpaysuccesscount},#{dateval},#{payoutrate},#{todayorder},#{successorder},#{todayorderamount},#{todaysuccessorderamount},#{type},#{create_by},#{create_time},#{update_by},#{update_time},#{remark})
	</insert>
	<!-- batchSava -->
	<insert id="batchSava" parameterType="java.util.List">
		insert into
		systemstatisticalreports(id,tenant_id,userid,merchantid,todayincome,incomecount,payoutrate,todayorder,successorder,todayorderamount,todaysuccessorderamount,type,create_by,create_time,update_by,update_time,remark)
		values
		<foreach collection="list" item="item" index="index"
			separator=",">
			(#{id},#{tenant_id},#{userid},#{merchantid},#{todayincome},#{incomecount},#{payoutrate},#{todayorder},#{successorder},#{todayorderamount},#{todaysuccessorderamount},#{type},#{create_by},#{create_time},#{update_by},#{update_time},#{remark})
		</foreach>
	</insert>
	<!-- delById -->
	<delete id="delById" parameterType="java.lang.Long">
		delete from
		systemstatisticalreports
		where id = #{id}
	</delete>
	<!-- update -->
	<update id="put"
		parameterType="com.yt.app.api.v1.entity.Systemstatisticalreports">
		update systemstatisticalreports
		<set>
			<if test="tenant_id != null">
				tenant_id= #{tenant_id},
			</if>
			<if test="userid != null">
				userid= #{userid},
			</if>
			<if test="merchantid != null">
				merchantid= #{merchantid},
			</if>
			<if test="todayincome != null">
				todayincome= #{todayincome},
			</if>
			<if test="incomecount != null">
				incomecount= #{incomecount},
			</if>
			<if test="payoutrate != null">
				payoutrate= #{payoutrate},
			</if>
			<if test="todayorder != null">
				todayorder= #{todayorder},
			</if>
			<if test="successorder != null">
				successorder= #{successorder},
			</if>
			<if test="todayorderamount != null">
				todayorderamount= #{todayorderamount},
			</if>
			<if test="todaysuccessorderamount != null">
				todaysuccessorderamount= #{todaysuccessorderamount},
			</if>
			<if test="type != null">
				type= #{type},
			</if>
			<if test="create_by != null">
				create_by= #{create_by},
			</if>
			<if test="create_time != null">
				create_time= #{create_time},
			</if>
			<if test="update_by != null">
				update_by= #{update_by},
			</if>
			<if test="update_time != null">
				update_time= #{update_time},
			</if>
			<if test="remark != null">
				remark= #{remark},
			</if>
			<if test="version != null">
				version= #{version}+1
			</if>
		</set>
		where id = #{id} 
	</update>
	<!--getById -->
	<select id="get" parameterType="java.lang.Long"
		resultMap="ResultMap">
		select
		<include refid="Base_Column_List" />
		from systemstatisticalreports
		where id = #{id}
	</select>
	<!-- getList -->
	<select id="list" parameterType="java.util.HashMap"
		resultMap="ResultMap">
		select
		<include refid="Base_Column_List" />
		from systemstatisticalreports
		<where>
			1=1
			<if test="stime != null and etime != ''">
				and dateval BETWEEN  #{stime} AND  #{etime}
			</if>
		</where>
	</select>
	<!-- getMap -->
	<select id="page" parameterType="java.util.HashMap"
		resultMap="ResultMapVO">
		select
		<include refid="Base_Column_List" />
		from systemstatisticalreports
		<where>
			1=1
			<if test="name != null and name != ''">
				and name LIKE CONCAT( '%', #{name} , '%' )
			</if>
			<if test="orderBy != null and dir != null">
				order by ${orderBy} ${dir}
			</if>
			<if test="pageStart != null and pageEnd != null">
				LIMIT #{pageStart},#{pageEnd}
			</if>
		</where>
	</select>
	<!-- getCount -->
	<select id="countlist" parameterType="java.util.HashMap"
		resultType="int">
		select count(*) from systemstatisticalreports
		<where>
			1=1
			<if test="name != null and name != ''">
				and name LIKE CONCAT( '%', #{name} , '%' )
			</if>
		</where>
	</select>
	<!-- listByArrayId -->
	<select id="listByArrayId" parameterType="java.util.HashMap"
		resultMap="ResultMap">
		select
		<include refid="Base_Column_List" />
		from systemstatisticalreports
		<where>
			1=1
			<if test="array != null and array.length > 0">
				and id in
				<foreach item="item" index="index" collection="array"
					open="(" separator="," close=")">
					#{item}
				</foreach>
			</if>
		</where>
	</select>
</mapper>